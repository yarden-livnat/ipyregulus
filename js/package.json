{
  "name": "@regulus/ipyregulus",
  "version": "0.8.5",
  "description": "Regulus widgets for JupyterLab",
  "author": "Yarden Livnat",
  "main": "lib/labext",
  "typings": "lib/index.d.ts",
  "keywords": [
    "jupyter",
    "widgets"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "files": [
    "lib/**/*.{d.ts,js,css,scss,html}",
    "schema/*.json"
  ],
  "scripts": {
    "clean": "rimraf docs && rimraf lib && rimraf tsconfig.tsbuildinfo",
    "build": "webpack -d",
    "prepare": "npm run clean && npm run build",
    "watch": "webpack -d --watch"
  },
  "devDependencies": {
    "@types/backbone": "^1.4.1",
    "@types/d3": "^5.7.2",
    "@types/ndarray": "^1.0.7",
    "@types/node": "^12.0.8",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "@jupyterlab/application": "^1.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "d3": "^5.9.2",
    "d3-scale-chromatic": "^1.3.3",
    "d3-tip": "^0.9.1",
    "file-loader": "^4.0.0",
    "jupyter-dataserializers": "^2.1.0",
    "jupyter-datawidgets": "^5.2.0",
    "jupyter-scales": "^3.0.0",
    "kernel-smooth": "^0.2.3",
    "ndarray": "^1.0.18",
    "raw-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "validate.io-array": "^1.0.6",
    "validate.io-function": "^1.0.2"
  }
}
