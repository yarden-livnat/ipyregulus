{
  "name": "@regulus/ipyregulus",
  "version": "0.7.0",
  "description": "Regulus widgets for JupyterLab",
  "author": "Yarden Livnat",
  "main": "src/plugin.ts",
  "types": "lib/labext.d.ts",
  "keywords": [
    "jupyter",
    "widgets"
  ],
  "jupyterlab": {
    "extension": "lib/labext"
  },
  "files": [
    "lib/**/*.{d.ts,js,css,scss,html}"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build:lib": "webpack -d",
    "build:labextension": "cd ../ipyregulus && rimraf labextension && mkdir labextension&& cd labextension && npm pack ../../js",
    "foo" : "rimraf dist && mkdirp dist && cd dist && npm pack ..",
    "build": "npm run build:lib && npm run build:labextension",
    "watch": "webpack -d --watch"
  },
  "devDependencies": {
    "@types/backbone": "^1.4.1",
    "@types/d3": "^5.7.2",
    "@types/ndarray": "^1.0.7",
    "@types/node": "^12.0.8",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "@jupyterlab/application": "^1.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "d3": "^5.9.2",
    "d3-scale-chromatic": "^1.3.3",
    "d3-tip": "^0.9.1",
    "file-loader": "^4.0.0",
    "jupyter-dataserializers": "^2.1.0",
    "jupyter-datawidgets": "^5.2.0",
    "jupyter-scales": "^3.0.0",
    "ndarray": "^1.0.18",
    "raw-loader": "^3.0.0",
    "style-loader": "^0.23.1"
  }
}
